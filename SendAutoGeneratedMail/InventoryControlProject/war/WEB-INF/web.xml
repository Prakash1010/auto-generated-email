<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">

  <!-- Servlets -->
  <servlet>
		<servlet-name>product</servlet-name>
		<servlet-class>com.edu.inventorycontrol.server.ProductServiceImpl</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>product</servlet-name>
		<url-pattern>/product</url-pattern>
	</servlet-mapping>
  
   <servlet>
		<servlet-name>inventorystock</servlet-name>
		<servlet-class>com.edu.inventorycontrol.server.InventoryStockServiceImpl</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>inventorystock</servlet-name>
		<url-pattern>/inventorystock</url-pattern>
	</servlet-mapping>
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>InventoryControlProject.html</welcome-file>
  </welcome-file-list>

</web-app>

<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:FormPanel ui:field="formPanel" width="200px">
			<g:VerticalPanel ui:field="vPanel">
				<g:HorizontalPanel ui:field="hPanel">
					<g:cell>
						<g:FileUpload ui:field="uploader" name="myFile"
							width="200px"></g:FileUpload>
					</g:cell>
				</g:HorizontalPanel>
			</g:VerticalPanel>
		</g:FormPanel>
		<g:Button ui:field="btnSubmit" text="Submit" />
	</g:HTMLPanel>
</ui:UiBinder> 
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel>

		<g:FormPanel ui:field="formPanel" width="200px">
			<g:VerticalPanel>
				<g:Label>Upload Log file here: </g:Label>
				<g:FileUpload ui:field="uploader" name="myFile" width="200px"></g:FileUpload>
			</g:VerticalPanel>
		</g:FormPanel>
		<br />
		<br />
		<g:Button ui:field="btnSubmit" text="Submit" />

	</g:HTMLPanel>
</ui:UiBinder> 